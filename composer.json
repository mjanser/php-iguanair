{
    "name": "mjanser/iguanair",
    "type": "library",
    "description": "Library for sending IR signals using iguanaIR devices",
    "keywords": ["iguanaIR"],
    "homepage": "https://github.com/mjanser/iguana",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Martin Janser",
            "email": "martin@duss-janser.ch"
        }
    ],
    "support": {
        "source": "https://github.com/mjanser/iguana",
        "issues": "https://github.com/mjanser/iguana/issues"
    },
    "require": {
        "php": ">=5.6",
        "symfony/process": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "fabpot/php-cs-fixer": "^1.11",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": { "IguanaIr\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "IguanaIr\\Test\\": "tests/" }
    }
}

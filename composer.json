{
    "name": "mjanser/iguanair",
    "type": "library",
    "description": "Library for sending IR signals using iguanaIR devices",
    "keywords": ["iguanaIR"],
    "homepage": "https://github.com/mjanser/php-iguana",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Martin Janser",
            "email": "martin@duss-janser.ch"
        }
    ],
    "support": {
        "source": "https://github.com/mjanser/php-iguana",
        "issues": "https://github.com/mjanser/php-iguana/issues"
    },
    "require": {
        "php": ">=7.0",
        "symfony/process": "^3.0|^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "IguanaIr\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
